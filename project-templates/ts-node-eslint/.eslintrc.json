{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "airbnb-base"
  ],
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-unused-vars": "warn",
    "linebreak-style": "off",
    "no-plusplus": "off",
    "import/no-unresolved": "off",
    "import/extensions": "off"
  }
}
